<div class="row">
  <div class="col-9">
    <table class="table table-bordered table-hover">
      <thead>
        <tr>
          <th scope="col">Ticker</th>
          <th scope="col">Name</th>
          <th scope="col">Price</th>
          <th scope="col">Action</th>
        </tr>
      </thead>
      <tbody>
        <% @stocks.each do |s| %>
        <tr>
          <th scope="row"><%= s.ticker %></th>
          <td><%= s.name %></td>
          <td><%= s.last_price %></td>
          <td>Action To Do</td>
        </tr>
         <% end %>
      </tbody>
    </table>

  </div>
</div>
